{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    // react
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "arrow-function"
      }
    ],
    // next
    "@next/next/no-html-link-for-pages": "off"
  },
  "ignorePatterns": [
    "**/*.js",
    "**/*.json",
    "node_modules",
    "public",
    "styles",
    ".next",
    "coverage",
    "dist"
  ]
}
